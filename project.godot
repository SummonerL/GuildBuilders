; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="A Test of Artistry"
run/main_scene="res://Scenes/Overworld/Overworld_Scene.tscn"
config/icon="res://icon.png"

[autoload]

Game_Constants="*res://Autoload/Game_Constants.gd"
Game_Config="*res://Autoload/Game_Config.gd"
Player_Globals="*res://Autoload/Player_Globals.gd"
Player_Party="*res://Entities/Player/Party.tscn"
Items="*res://Autoload/Items.gd"
Abilities="*res://Autoload/Abilities.gd"
Actions="*res://Autoload/Actions.gd"
Signal_Manager="*res://Autoload/Signal_Manager.gd"

[debug]

settings/fps/force_fps=30

[display]

window/size/width=640
window/size/height=576
window/dpi/allow_hidpi=true
window/vsync/vsync_via_compositor=true
window/stretch/mode="viewport"
window/stretch/aspect="keep"
window/stretch/shrink=4.0

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
quality/dynamic_fonts/use_oversampling=false
